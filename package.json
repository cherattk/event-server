{
    "name": "eventset",
    "version": "1.3.3",
    "description": "Topic-based Pub-Sub Implementation",
    "author": "Karim Cheratt",
    "keywords": ["eventset" , "pub-sub" , "event-driven" , "publish-subscrib", "topic-based"],
    "license": "MIT",
    "main": "index.js",
    "babel": {
        "presets": [
            "env"
        ]
    },
    "scripts": {
        "build": "npm run build:app && npm run test",
        "build:app"    : "rimraf ./build/* && babel ./src --out-dir ./build --source-maps",
        "test"         : "npm run build:test && npm run test:eventset",
        "build:test"   : "rimraf ./test/build/* && babel ./test/src --out-dir ./test/build --source-maps",
        "test:eventset": "mocha --colors test/build/eventset.test.js",
        "build:demo"   : "cd demo/ && npm install && npm run build",
        "npm:publish"   : "node build.package.js && cd build/ && npm publish"
    },
    "devDependencies": {
        "babel-cli": "6.26.0",
        "babel-preset-env": "1.6.1",
        "mocha": "5.0.5",
        "rimraf": "2.6.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cherattk/eventset.git"
    },
    "bugs": {
        "url": "https://github.com/cherattk/eventset/issues"
    },
    "homepage": "https://github.com/cherattk/eventset#readme"
}
